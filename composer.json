{
    "name": "bleuren/voyager-bread-generator",
    "description": "Create BREAD from the console using artisan",
    "tags": ["voyager", "bread generator"],
    "type": "library",
    "version": "1.1.0",
    "require": {
        "tcg/voyager": "^1.0"
    },
    "require-dev": {
        "laravel/laravel": "^8.0",
        "larapack/hooks": "~1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bleu Ren",
            "email": "bleuren@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "VoyagerBread\\": "src/"
        }
      },
      "extra": {
        "hook": {
          "providers": [
            "VoyagerBread\\BreadGeneratorServiceProvider"
          ]
        },
        "laravel": {
            "providers": [
                "VoyagerBread\\BreadGeneratorServiceProvider"
            ]
        }
      }
}
